{
  "builds": [
    {
      "src": "kuandorwear/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "kuandorwear/manage.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      },
      "functions": [
        {
          "handler": "manage.superuser"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "kuandorwear/wsgi.py"
    }
  ]
}


